{
  "name": "tropicalista/admin",
  "description": "A boilerplate user administration for adminlte",
  "license": "MIT",
  "keywords": [
    "admin"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Francesco Pepe",
      "email": "francescopepe@email.it"
    }
  ],
  "require": {
    "php": ">=7.0",
    "jeroennoten/laravel-adminlte": "*",
    "spatie/laravel-permission": "^2.7",
    "yajra/laravel-datatables-oracle": "~8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Tropicalista\\Admin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tropicalista\\Admin\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tropicalista\\Admin\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
